{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "测试",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/src/main.lua",
            "cwd": "${workspaceRoot}/bin",
            "path": "${workspaceRoot}/bin/?.lua",
            "cpath": "${workspaceRoot}/bin/?.dll",
            "arg": [
                "${workspaceRoot}/war3map.j"
            ],
            "consoleCoding": "utf8",
            "sourceCoding": "utf8"
        },
        {
            "name": "单元测试",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/src/main.lua",
            "cwd": "${workspaceRoot}",
            "env": {
                "PATH": "${workspaceRoot}\\bin\\"
            },
            "luaRuntime": "5.4 32bit",
            "path": "${workspaceRoot}/bin/?.lua",
            "cpath": "${workspaceRoot}/bin/?.dll;",
            "arg": [
            ],
            "consoleCoding": "utf8",
            "sourceCoding": "utf8"
        },
        {
            "name": "临时",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/src/temp.lua",
            "cwd": "${workspaceRoot}",
            "env": {
                "PATH": "${workspaceRoot}\\bin\\"
            },
            "luaexe": "${workspaceRoot}\\bin\\lua.exe",
            "luadll": "${workspaceRoot}\\bin\\lua54.dll",
            "path": "${workspaceRoot}/src/?.lua",
            "cpath": "${workspaceRoot}/bin/?.dll",
            "arg": [
            ],
            "consoleCoding": "utf8",
            "sourceCoding": "utf8"
        },
    ]
}
